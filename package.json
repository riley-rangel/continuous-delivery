{
  "name": "continuous-delivery",
  "version": "1.0.0",
  "engines": {
    "node": "8.8.0"
  },
  "description": "Setting up continuous integration with Travis CI.",
  "main": "index.js",
  "scripts": {
    "lint": "run-p lint:*",
    "lint:html": "htmlhint ./**/*.html",
    "lint:css": "stylelint ./**/*.css",
    "lint:js": "eslint ./**/*.js",
    "test": "mocha test",
    "precommit": "npm test",
    "bundle": "browserify client/index.js -t babelify -o server/public/main.js",
    "bundle:watch": "watchify client/index.js -t babelify -o server/public/main.js",
    "cover": "nyc --reporter=lcov --reporter=text npm test",
    "start": "node .",
    "watch": "nodemon server/."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/riley-rangel/continuous-delivery.git"
  },
  "author": "riley-rangel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/riley-rangel/continuous-delivery/issues"
  },
  "homepage": "https://github.com/riley-rangel/continuous-delivery#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "mongodb": "^2.2.33",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "chai": "^4.1.2",
    "eslint": "4.5.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.3.0",
    "eslint-plugin-standard": "3.0.1",
    "htmlhint": "0.9.13",
    "husky": "0.14.3",
    "mocha": "^4.0.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "4.1.1",
    "nyc": "^11.2.1",
    "request": "^2.83.0",
    "stylelint": "8.0.0",
    "stylelint-config-standard": "17.0.0",
    "watchify": "^3.9.0"
  }
}
